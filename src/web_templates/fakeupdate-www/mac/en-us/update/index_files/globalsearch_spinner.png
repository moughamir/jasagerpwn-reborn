	<script type="text/javascript">
		function absoluteURLForValue(href) {
			if(!!href && href.indexOf("http") !== 0) {
				return window.location.protocol+"//"+window.location.host+ ((href.indexOf("/") !== 0) ? "/" : "")+href;
			}
			return href;
		}

		function getRandomElement(a) {
			return a[Math.floor(Math.random()*a.length)];
		}

		function setiOSClassName(classname){
			document.getElementById(iosPromoId).className = classname;
		};

		if (document.getElementById('headerpromos').innerHTML == '') {
			var promos = {
				ipad2white: { title:'iPad 2. Thinner. Lighter. Faster. FaceTime. Smart Covers. 10-hour battery life.', href:'/ipad/', img:'./startpage/images/promo_ipad2_white20110315.jpg' },
				ipadsmartcover: { title:'iPad Smart Cover. The perfect-fit cover that wakes up, stands up, and brightens up iPad 2.', href:'/ipad/smart-cover/', img:'./startpage/images/promo_ipad2_smartcover20110329.jpg' },
				teachforamerica: { title:'Help teach kids a thing or two. Donate your old iPad to Teach for America at any Apple Retail Store.', href:'/retail/', img:'./startpage/images/promo_teachforamerica20110405.jpg' },
				macbookpro: { title:'The New MacBook Pro. state-of-the-art processors. All-new graphics. Breakthrough high-speed I/O.', href:'/macbookpro/', img:'./startpage/images/promo_macbookpro20110315.jpg' },
				whiteiphone: { title:'Finally. The amazing iPhone 4. Now available in white.', href:'/iphone/', img:'./startpage/images/promo_white_iphone_20110428.jpg' }
			};
			
			var promosets = [
				[ promos.ipad2white, promos.ipadsmartcover, promos.whiteiphone, promos.macbookpro ]
			];

			var promoset = getRandomElement(promosets);
			for (var i=0; i<promoset.length; i++) {
				var promo = promoset[i], promoHref = absoluteURLForValue(promo.href);
					document.getElementById('headerpromos').innerHTML += '<a id="promo'+parseInt(i+1)+'" href="'+promoHref+'" title="'+promo.title+'"><img src="'+promo.img+'" width="'+(promo.width||236)+'" height="155" alt="'+promo.title+'" /><\/a>';
			}
		}

		// store promos

		var storeWrapper = document.getElementById('sb-store').getElementsByTagName('ul')[0];
		var storeNodes = storeWrapper.getElementsByTagName('li');
		var storePromos = [];
		for (var i=storeNodes.length-1; i>=0; i--) {
			storePromos.push(storeNodes[i].parentNode.removeChild(storeNodes[i]));
		}

		var storeStart = Math.floor(Math.random()*storePromos.length);

		for (var i=storeStart-1; i>=0; i--) {
			storeWrapper.appendChild(storePromos[i]);
			delete storePromos[i];
		}
		for (var i=storePromos.length-2; i>=0; i--) {
			if (storePromos[i]) {
				storeWrapper.appendChild(storePromos[i]);
				delete storePromos[i];
			}
		}

		var storeNodes = storeWrapper.getElementsByTagName('li');
		var storeNodeLength = storeNodes.length;
		for (var i=3; i<storeNodeLength; i++) {
			storeWrapper.removeChild(storeNodes[i]);
		}

		// featured tutorial
		var tutorials = [
			{ title:'Anatomy of a Mac', href:'/findouthow/mac/tutorials/anatomy.html#anatomy', img:'./startpage/images/foh-anatomy-20110113.jpg' },
			{ title:'Wireless Basics', href:'/findouthow/mac/tutorials/wirelessbasics.html#wirelessbasics', img:'./startpage/images/foh-wirelessbasics-20090327.jpg' },
			{ title:'Parental Controls', href:'/findouthow/mac/tutorials/parentalcontrols.html#parentalcontrols', img:'./startpage/images/foh-parental-20090327.jpg' },
			{ title:'PC to Mac: The Basics', href:'/findouthow/mac/tutorials/pctomac.html#pctomac', img:'./startpage/images/foh-switcher-20090327.jpg' },
			{ title:'Streaming Music Wirelessly', href:'/findouthow/mac/tutorials/wirelessmusic.html#wirelessmusic', img:'./startpage/images/foh-wirelessmusic-20090327.jpg' },
			{ title:'Time Machine Basics', href:'/findouthow/mac/tutorials/timemachinebasics.html#timemachinebasics', img:'./startpage/images/foh-leopardtimemachine-20090109.jpg'}
		];
		var tutorial = getRandomElement(tutorials), tutorialHref = tutorial.href;
		tutorialHref = absoluteURLForValue(tutorial.href);
		document.getElementById('sb-t-featured').innerHTML = '<a class="featured" href="'+tutorialHref+'"><img src="'+tutorial.img+'" width="124" height="62" alt="Featured Tutorial: '+tutorial.title+'" />Featured Tutorial:<br /><strong>'+tutorial.title+'</strong></a>';

	</script>
